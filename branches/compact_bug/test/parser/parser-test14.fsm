machine top_level
{
   event e1, e2, e3;
   state s1, s2, s3;

		machine sub_machine1
		{
		   event e1, e2, e3;
		   state s1, s2, s3;
		
		   action a1[e1, s1] transition s2;
		   action a2[e2, s2] transition s3;
		   action a3[e3, s3] transition s1;
		}
		
		machine sub_machine2
		{
		   event e1, e2, e3;
		   state s1, s2, s3;
		
		   action a1[e1, s1] transition s2;
		   action a2[e2, s2] transition s3;
		   action a3[e3, s3] transition s1;
		}
		
		machine sub_machine3
		{
		   event e1, e2, e3;
		   state s1, s2, s3;
		
		   action a1[e1, s1] transition s2;
		   action a2[e2, s2] transition s3;
		   action a3[e3, s3] transition s1;
		}
		
   action a1[e1,s1] transition s2;

}

