machine test_fsm
{
	event e1, e2, e3, e4;
	state s1, s2;

	action a1[e1, s1];
	action a2[e2, s1] transition s2;

	action a3[e3, s2];
	transition [e2, s2] s1;

	action a4[e4, (s1, s2)];

	a1 returns e4, noEvent;
	a2 returns e4, noEvent;

	a3 returns noEvent;

	a4 returns e1;

	sequence e1, e2, e4;
}

